#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/voicefirstai/Projects/BASE/docs-minikit/node_modules/.pnpm/vocs@1.0.11_@types+node@22.15.3_@types+react@19.1.2_acorn@8.14.1_jiti@2.4.2_lightningcs_9ee27f37b11664c85f15df7b62429cfd/node_modules/vocs/_lib/cli/node_modules:/Users/voicefirstai/Projects/BASE/docs-minikit/node_modules/.pnpm/vocs@1.0.11_@types+node@22.15.3_@types+react@19.1.2_acorn@8.14.1_jiti@2.4.2_lightningcs_9ee27f37b11664c85f15df7b62429cfd/node_modules/vocs/_lib/node_modules:/Users/voicefirstai/Projects/BASE/docs-minikit/node_modules/.pnpm/vocs@1.0.11_@types+node@22.15.3_@types+react@19.1.2_acorn@8.14.1_jiti@2.4.2_lightningcs_9ee27f37b11664c85f15df7b62429cfd/node_modules/vocs/node_modules:/Users/voicefirstai/Projects/BASE/docs-minikit/node_modules/.pnpm/vocs@1.0.11_@types+node@22.15.3_@types+react@19.1.2_acorn@8.14.1_jiti@2.4.2_lightningcs_9ee27f37b11664c85f15df7b62429cfd/node_modules:/Users/voicefirstai/Projects/BASE/docs-minikit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/voicefirstai/Projects/BASE/docs-minikit/node_modules/.pnpm/vocs@1.0.11_@types+node@22.15.3_@types+react@19.1.2_acorn@8.14.1_jiti@2.4.2_lightningcs_9ee27f37b11664c85f15df7b62429cfd/node_modules/vocs/_lib/cli/node_modules:/Users/voicefirstai/Projects/BASE/docs-minikit/node_modules/.pnpm/vocs@1.0.11_@types+node@22.15.3_@types+react@19.1.2_acorn@8.14.1_jiti@2.4.2_lightningcs_9ee27f37b11664c85f15df7b62429cfd/node_modules/vocs/_lib/node_modules:/Users/voicefirstai/Projects/BASE/docs-minikit/node_modules/.pnpm/vocs@1.0.11_@types+node@22.15.3_@types+react@19.1.2_acorn@8.14.1_jiti@2.4.2_lightningcs_9ee27f37b11664c85f15df7b62429cfd/node_modules/vocs/node_modules:/Users/voicefirstai/Projects/BASE/docs-minikit/node_modules/.pnpm/vocs@1.0.11_@types+node@22.15.3_@types+react@19.1.2_acorn@8.14.1_jiti@2.4.2_lightningcs_9ee27f37b11664c85f15df7b62429cfd/node_modules:/Users/voicefirstai/Projects/BASE/docs-minikit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vocs/_lib/cli/index.js" "$@"
else
  exec node  "$basedir/../vocs/_lib/cli/index.js" "$@"
fi
